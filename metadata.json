"name": "almariah-s3file",
"version": "0.1.0",
"author": "Abdullah Almariah",
"summary": "This module uses unicorns to fetch files from s3.",
"license": "Apache-2.0",
"source": "https://github.com/almariah/puppet-s3",
"project_page": "https://github.com/almariah/puppet-s3",
"issues_url": "https://github.com/almariah/puppet-s3/issues",
"requirements": [
  {
    "name": "puppet",
    "version_requirement": ">3.0.0 <6.0.0"
  }
],
"dependencies": [
  {
    "name": "puppetlabs/stdlib",
    "version_requirement": ">= 4.5.0 <5.0.0"
  }
],
"operatingsystem_support": [
  {
    "operatingsystem": "Debian",
    "operatingsystemrelease": [
      "6",
      "7"
    ]
  },
  {
    "operatingsystem": "RedHat",
    "operatingsystemrelease": [
      "5",
      "6",
      "7"
    ]
  }
]
}
